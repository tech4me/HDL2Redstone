library(HDL2Redstone) {
  cell(BUF) {
    pin(A) {
      direction: input;
    }
    pin(Y) {
      direction: output;
      function: "A";
    }
  }
  cell(NOT) {
    pin(A) {
      direction: input;
    }
    pin(Y) {
      direction: output;
      function: "A'";
    }
  }
/*
  cell(NAND) {
    pin(A) {
      direction: input;
    }
    pin(B) {
      direction: input;
    }
    pin(Y) {
      direction: output;
      function: "(A*B)'";
    }
  }
*/
  cell(AND) {
    pin(A) {
      direction: input;
    }
    pin(B) {
      direction: input;
    }
    pin(Y) {
      direction: output;
      function: "(A*B)";
    }
  }
}
